<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="iptables" name="com.hzih.stp.domain.Iptables">
    <id access="field" name="id" type="long" column="id" length="11">
      <generator class="increment"/>
    </id>
    <property name="clientIp" length="20" column="clientIp" access="field" type="string"/>
    <property name="serverIp" length="100" column="serverIp" access="field" type="string"/>
    <property name="port" column="port" access="field" type="int"/>
    <property name="isActive" column="isActive" access="field" type="boolean"/>
  </class>
</hibernate-mapping>
