<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="interface_htjk" name="com.hzih.stp.domain.Htjk">
    <id access="field" name="id" type="int" column="id" length="11">
      <generator class="increment"/>
    </id>
    <property name="info" length="50" column="info" access="field" type="string"/>
    <property name="name" length="50" column="name" access="field" type="string"/>
    <property name="status"  length="4" column="status" access="field" type="int"/>
  </class>
</hibernate-mapping>
